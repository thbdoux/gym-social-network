{
  "expo": {
    "name": "dou-mobile",
    "slug": "dou-social-network-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "dou",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.doucompany.dou",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription":      "Allow %@ to pick images for your posts.",
        "NSPhotoLibraryAddUsageDescription":   "Allow %@ to save the edited picture.",
        "NSCameraUsageDescription":            "Allow %@ to take a new photo.",
        "NSMicrophoneUsageDescription":        "Needed for video posts with sound." 
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.doucompany.dou"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.38465928219-ouf41lc4o75ruaspr62s7o0bqr99fi22",
          "webClientId": "38465928219-lnt8gkdkhn1id2vh93a54j41h8516op9.apps.googleusercontent.com"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Let %@ access your photos."
        }
      ]

    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2297eb90-1775-4516-8c5c-3f6f0fe9b1d4"
      }
    }
  }
}
